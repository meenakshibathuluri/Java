public class EmployeeWithConstructors {
    private int id;
    private String name;
    private int salary;
    Employee() {

    }
    Employee(int id, String name, int salary) {
        this.id = id;
        this.name = name;
        this.salary = salary;
    }
    Employee(int id, String name) {
        this.id = id;
        this.name = name;
    }
    Employee(int salary) {
        this.salary = salary;
    }
    Employee(int id, int salary) {
        this.id = id;
        this.salary = salary;
    }
    public void setId(int id) {
        this.id = id;
    }
    public void setName(String name) {
        this.name = name;
    }
    public void setSalary(int salary) {
        this.salary = salary;
    }
    public int getId() {
        return id;
    }
    public String getName() {
        return name;
    }
    public int getSalary() {
        return salary;
    }
    public String toString() {
        return "Id is : "+this.id+", Name is: "+this.name+", Salary is: "+this.salary;

    }
}
